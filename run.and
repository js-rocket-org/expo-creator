#!/bin/sh

LOCAL_EXPO=./node_modules/.bin/expo

android_start() {
  $LOCAL_EXPO run:android
}


android_build() {
  eas build --platform android --profile preview --local --non-interactive
}


if [ "$1" = "start" ]; then android_start ; exit $? ;fi
if [ "$1" = "build" ]; then android_build $2 ; exit $? ;fi
